    LOAD R0, 0x00, 10      ; R0 = 10
    LOAD R1, 0x00, 5       ; R1 = 5
    LOAD R2, 0x00, 2       ; R2 = 2
    LOAD R3, 0x00, 3       ; R3 = 3
    
    ADD R4, R0, R1         ; R4 = 10 + 5 = 15
    MUL R5, R4, R2         ; R5 = 15 * 2 = 30
    SUB R6, R5, R3         ; R6 = 30 - 3 = 27
    
    DBG
    HALT